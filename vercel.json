{
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "python app.py",
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  },
  "env": {
    "FLASK_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
